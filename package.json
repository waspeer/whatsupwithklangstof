  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "code:check:rules": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "code:check": "yarn code:lint; yarn code:format --check",
    "code:clean": "yarn code:lint --fix; yarn code:format --write",
    "code:format": "prettier './{src,test}/**/**.{js,jsx,ts,tsx}'",
    "code:lint": "eslint --ext .js,.jsx,.ts,.tsx 'src/' 'test/'",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "graphql:generate-types": "apollo codegen:generate src/types/__generated__/ -c=apollo-config.js  --outputFlat --target=typescript",
    "serve": "gatsby serve -p 8000",
    "start": "yarn develop",
    "deploy": "surge public whatsupwith.klangstof.com",
    "test:e2e:dev": "CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e": "start-server-and-test serve http://localhost:8000 cy:run",
